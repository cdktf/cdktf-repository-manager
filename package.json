{
  "name": "repos",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "format": "prettier --write '**/*.{ts,js,md,yml}'",
    "get": "cdktf get",
    "build": "yarn get && tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "echo ok",
    "lint": "npx lint-staged",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "output": "cd cdktf.out && terraform output -json -state ../terraform.tfstate",
    "repos": "yarn -s output | jq  'with_entries(select(.key|match(\"providerRepos\";\"i\")))[] | .value'"
  },
  "engines": {
    "node": ">= 20.9.0"
  },
  "dependencies": {
    "@cdktf/provider-github": "^15.0.0",
    "cdktf": "^0.21.0",
    "cdktf-cli": "^0.21.0",
    "change-case": "~4.1.2",
    "constructs": "^10.4.2"
  },
  "devDependencies": {
    "@types/node": "~20",
    "@typescript-eslint/eslint-plugin": "^8",
    "@typescript-eslint/parser": "^8",
    "eslint": "^9",
    "lint-staged": "^15.5.2",
    "prettier": "^3",
    "tsx": "^4.0.0",
    "typescript": "~5.8.0"
  },
  "lint-staged": {
    "*.{md,yml}": "prettier --write",
    "**/*.{ts,js}": [
      "eslint",
      "prettier --write"
    ]
  },
  "terraform": {
    "version": "1.13.2"
  }
}
