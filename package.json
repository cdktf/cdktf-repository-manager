{
  "name": "repos",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "format": "prettier --write '**/*.{ts,js,md,yml}'",
    "get": "cdktf get",
    "build": "yarn get && tsc",
    "synth": "cdktf synth",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "echo ok",
    "lint": "eslint **/*.{ts,js}",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "output": "cd cdktf.out && terraform output -json -state ../terraform.tfstate",
    "repos": "yarn -s output | jq  'with_entries(select(.key|match(\"providerRepos\";\"i\")))[] | .value'",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=10.12"
  },
  "dependencies": {
    "@cdktf/provider-github": "^2.0.10",
    "cdktf": "^0.12.0",
    "cdktf-cli": "^0.12.0",
    "change-case": "^4.1.2",
    "constructs": "^10.0"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.27.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.7.1",
    "typescript": "^3.9.7"
  },
  "lint-staged": {
    "*.{md,yml}": "prettier --write",
    "**/*.{ts,js}": [
      "eslint",
      "prettier --write"
    ],
    ".github/lib/**": [
      "eslint"
    ]
  }
}
